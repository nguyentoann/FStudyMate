{
  "extends": ["react-app"],
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "jsx-a11y/anchor-is-valid": "warn",
    "jsx-a11y/heading-has-content": "warn",
    "jsx-a11y/anchor-has-content": "warn",
    "no-useless-escape": "warn",
    "default-case": "warn",
    "import/no-anonymous-default-export": "warn"
  }
} 